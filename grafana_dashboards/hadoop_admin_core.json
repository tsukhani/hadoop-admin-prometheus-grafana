{
  "annotations": {
    "list": []
  },
  "editable": true,
  "panels": [
    {
      "type": "graph",
      "title": "NN Heap Used (%)",
      "targets": [
        {
          "expr": "(jvm_memory_heap_used_bytes{instance=~\"nn1:7000\"} / jvm_memory_heap_max_bytes{instance=~\"nn1:7000\"}) * 100"
        }
      ]
    },
    {
      "type": "graph",
      "title": "DN1/2 JVM Threads",
      "targets": [
        {
          "expr": "jvm_threads{instance=~\"dn(1|2):7001|dn(1|2):7003\"}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Node CPU (nn1)",
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\",instance=\"nn1:9100\"}[5m])) * 100)"
        }
      ]
    }
  ],
  "schemaVersion": 36,
  "title": "Hadoop Admin \u2014 Core Metrics",
  "version": 1
}